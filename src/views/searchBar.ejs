
<!DOCTYPE html>
<html lang="es">
    <%- include('partials/head') %>
<body>
    <%- include('partials/header') %>

<% if (results.length) { %>
    <main class="main__search">
        <% results.forEach(result => { %>
            <%- include('partials/productCardS',{
                ...result,
                toThousand
            }) %>
        <% }) %>
    </main>
<% }else{ %>
    <main class="main__search__error">
    <div class="main__search__error--404" >No se encontro tu busqueda</div>
    </main>
    <% } %>

    <%- include('partials/footer') %>
</body>
</html>
